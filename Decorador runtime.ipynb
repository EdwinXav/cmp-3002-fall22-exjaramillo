    "#Creaci√≥n del decorador con un pause de 10^6 segundos\n",
    "def timer_func(func):\n",
    "    def wrap_func(*args, **kwargs):\n",
    "        t1 = time()*10**6\n",
    "        result = func(*args, **kwargs)\n",
    "        t2 = time()*10**6\n",
    "        t_total = t2-t1-10**6\n",
    "        return result, t_total\n",
    "    return wrap_func"
   ]
  }
